{
  "name": "ecommerce-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "node_modules/.bin/babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "node_modules/.bin/babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "node_modules/.bin/babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "node_modules/.bin/babel-node tools/build.js",
    "postbuild": "node_modules/.bin/babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AXA-GROUP-SOLUTIONS/ecommerce-boilerplate.git"
  },
